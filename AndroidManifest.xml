<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ucuccs.mycoursenote"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="14" />
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

   <application
        android:allowBackup="true"
        android:icon="@drawable/mm"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.ucuccs.mycoursenote.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.ucuccs.mycoursenote.CreateNotes"
            android:label="@string/title_activity_create_notes" >
        </activity>
        <activity
            android:name="edu.ucuccs.mycoursenote.Notes_Text"
            android:label="@string/title_activity_notes__text" >
        </activity>
        <activity
            android:name="edu.ucuccs.mycoursenote.Notes_Scribbling"
            android:label="@string/title_activity_notes__scribbling" >
        </activity>
        <activity
            android:name="edu.ucuccs.mycoursenote.Notes_Recording"
            android:label="@string/title_activity_notes__recording" >
        </activity>
        <activity
            android:name="edu.ucuccs.mycoursenote.Notes_Alarm"
            android:label="@string/title_activity_notes__alarm" >
        </activity>
    </application>

</manifest>
